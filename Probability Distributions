     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Mean: 0.613\n",
      "Variance: 0.237231\n",
      "Standard Deviation: 0.4870636508712183\n",
      "Median: 1.0\n",
      "Mode: ModeResult(mode=1, count=613)\n"
     ]
    }
   ],
   "source": [
    "mean = np.mean(data)\n",
    "variance = np.var(data)\n",
    "std = np.std(data)\n",
    "median = np.median(data)\n",
    "mode = st.mode(data)\n",
    "\n",
    "print(f'Mean: {mean}')\n",
    "print(f'Variance: {variance}')\n",
    "print(f'Standard Deviation: {std}')\n",
    "print(f'Median: {median}')\n",
    "print(f'Mode: {mode}')\n"
   ]
  },
