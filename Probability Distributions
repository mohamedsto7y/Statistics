  {
   "cell_type": "code",
   "execution_count": 374,
   "id": "5bd96a1e-6555-4c28-a190-6bf1115fea10",
   "metadata": {},
   "outputs": [],
   "source": [
    "np.random.seed(42)\n",
    "from scipy.stats import poisson\n",
    "lambda_=0.61\n",
    "p_0 = poisson.pmf(0,lambda_)\n",
    "p_1 = poisson.pmf(1,lambda_)\n",
    "p_2 = poisson.pmf(2,lambda_)\n",
    "p_3 = poisson.pmf(3,lambda_)\n",
    "p_4 = poisson.pmf(4,lambda_)\n",
    "p_5 = poisson.pmf(5,lambda_)\n",
    "p_6 = poisson.pmf(6,lambda_)\n"
   ]
  },
  {
   "cell_type": "code",
