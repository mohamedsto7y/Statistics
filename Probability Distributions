   ],
   "source": [
    "np.random.seed(42)\n",
    "data = np.random.binomial(n=1, p=0.5, size=50)\n",
    "plt.figure(figsize=(6, 4))\n",
    "sns.countplot(x=data,hue=data)\n",
    "plt.show()\n",
    "mean = np.mean(data)\n",
    "variance = np.var(data)\n",
    "std = np.std(data)\n",
    "median = np.median(data)\n",
    "mode = st.mode(data)\n",
    "\n",
    "print(f'Mean: {mean}')\n",
    "print(f'Variance: {variance}')\n",
    "print(f'Standard Deviation: {std}')\n",
    "print(f'Median: {median}')\n",
    "print(f'Mode: {mode}')\n",
    "s=bernoulli.pmf(1,p=0.5)\n",
    "f=bernoulli.pmf(0,p=0.5)\n",
    "print(f'P(1):{s}')\n",
    "print(f'P(0):{f:.1f}')\n",
    "sns.histplot(x=data,stat='proportion')"
   ]
  },
